class Point{	int x;	int y;		Point(int a,int b) {		x=a;		y=b;	}		int getX() {		return x;	}		int getY() {		return y;	}		void move(int a,int b) {		x=a;		y=b;	}		public String toString() {		return "p("+getX()+", "+getY()+")";	}}//Point p=new Point(3,5);//p.move(4,6);//System.out.println("p("+p.getX()+", "+p.getY()+")");class Circle{		Point p;	float radius;		Circle(Point p1, float r1){		p=p1;		radius = r1;	}		Point getCenter(){		return p;	}		float getRadius(){		return radius;	}		public String toString(){		return "Circle [center="+getCenter()+", radius="+getRadius()+"]";	}		void move(Point p1){		p=p1;	}		void enlarge(float r1){		radius =r1;	}}Point p= new Point(1,2);Circle c = new Circle(p, 1.5f);//c.getRadius();//c.toString();//c.move(new Point(3, 4));c.enlarge(2.5f);c.toString();